<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="700.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.prog2lab1arpan.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Pane prefHeight="300.0" prefWidth="450.0">
      <children>
         <TableView layoutX="230.0" layoutY="50.0" prefHeight="200.0" prefWidth="227.0">
           <columns>
             <TableColumn fx:id="id" prefWidth="75.0" text="ID" />
             <TableColumn fx:id="name" prefWidth="75.0" text="Name" />
               <TableColumn fx:id="sinNumber" prefWidth="75.0" text="SinNumber" />
           </columns>
         </TableView>
         <Button layoutX="318.0" layoutY="261.0" mnemonicParsing="false" text="Get data" onAction="#getData"/>
      </children>
    </Pane>
</VBox>
